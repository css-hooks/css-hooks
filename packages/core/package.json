{
  "name": "@css-hooks/core",
  "description": "CSS Hooks core library",
  "version": "3.0.1",
  "author": "Nick Saunders",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.39.4",
    "@types/color": "^3.0.6",
    "@types/node": "^22.7.5",
    "color": "^4.2.3",
    "csstype": "^3.1.3",
    "lightningcss": "^1.23.0",
    "puppeteer": "^22.0.0",
    "remeda": "^2.14.0",
    "rimraf": "^6.0.1",
    "typescript": "5.7.0-dev.20241102"
  },
  "files": [
    "cjs",
    "esm",
    "types",
    "tsdoc-metadata.json",
    "!**/*.test.*"
  ],
  "license": "MIT",
  "main": "cjs",
  "module": "esm",
  "type": "module",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/css-hooks/css-hooks.git",
    "directory": "packages/core"
  },
  "scripts": {
    "clean": "rimraf cjs esm types",
    "prepare": "tsc && tsc --project tsconfig.cjs.json && api-extractor run",
    "test": "node --experimental-strip-types --conditions @css-hooks/source --test src/index.test.ts"
  },
  "types": "types",
  "exports": {
    ".": {
      "@css-hooks/source": "./src/index.ts",
      "import": "./esm/index.js",
      "require": "./cjs/index.js",
      "types": "./types/index.d.ts"
    }
  }
}
